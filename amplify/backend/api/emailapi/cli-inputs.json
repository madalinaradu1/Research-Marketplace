{
  "version": 1,
  "paths": {
    "/send-email": {
      "name": "/send-email",
      "lambdaFunction": "sendEmailNotification",
      "permissions": {
        "setting": "open"
      }
    },
    "/delete-user": {
      "name": "/delete-user",
      "lambdaFunction": "adminDeleteUser",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}